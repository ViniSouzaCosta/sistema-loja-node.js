<%- include ('partials/navbar.ejs') %>
<%- include ('partials/header.ejs') %>
 
<div class="container">
    <div class="card">
        <div class="card-header">
            <h1>Clientes</h1>
        </div>
        <div class="card-body">
            <table class="table table-bordered trable-hover">
                <thead class="table-dark">
                    <tr>
                        <th>Nome:</th>
                        <th>CPF:</th>
                        <th>Endere√ßo:</th>
                    </tr>
                </thead>
                <tbody>
                    <!-- Chamando o Array de objetos-->
                    <% clientes.forEach(cliente => { %>
                    <tr>  
                        <td><%= cliente.nome %></td>
                        <td><%= cliente.cpf %></td>
                        <td><%= cliente.endereco %></td>
                    </tr>  
                    <% }) %>
                </tbody>
            </table>
        </div>
    </div>
</div>



